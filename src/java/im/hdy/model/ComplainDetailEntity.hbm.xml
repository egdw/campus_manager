<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.ComplainDetailEntity" table="COMPLAIN_DETAIL" schema="campus_travle">
        <id name="complainDetailId">
            <column name="COMPLAIN_DETAIL_ID" sql-type="bigint(20)"/>
        </id>
        <property name="complainId">
            <column name="COMPLAIN_ID" sql-type="bigint(20)" not-null="true"/>
        </property>
        <property name="complainLove">
            <column name="COMPLAIN_LOVE" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="complainWatch">
            <column name="COMPLAIN_WATCH" sql-type="bigint(20)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>