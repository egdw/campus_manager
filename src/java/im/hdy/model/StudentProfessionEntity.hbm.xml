<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.StudentProfessionEntity" table="STUDENT_PROFESSION" schema="campus_travle">
        <id name="professionId">
            <column name="PROFESSION_ID" sql-type="bigint(20)"/>
        </id>
        <property name="professionName">
            <column name="PROFESSION_NAME" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="collegeId">
            <column name="COLLEGE_ID" sql-type="int(20)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>