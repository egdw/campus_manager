<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.UserAuthrityEntity" table="USER_AUTHRITY" schema="campus_travle">
        <id name="userId">
            <column name="USER_ID" sql-type="bigint(20)"/>
        </id>
        <property name="authId">
            <column name="AUTH_ID" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>