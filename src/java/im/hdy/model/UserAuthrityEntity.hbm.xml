<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.UserAuthrityEntity" table="USER_AUTHRITY" schema="campus_travle">
        <composite-id mapped="true" class="im.hdy.model.UserAuthrityEntityPK">
            <key-property name="userId">
                <column name="USER_ID" sql-type="bigint(20)"/>
            </key-property>
            <key-property name="authId">
                <column name="AUTH_ID" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <many-to-one name="authorityByAuthId" class="im.hdy.model.AuthorityEntity">
            <column name="AUTH_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>