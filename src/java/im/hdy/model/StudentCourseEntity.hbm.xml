<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.StudentCourseEntity" table="STUDENT_COURSE" schema="campus_travle">
        <id name="courseid">
            <column name="COURSEID" sql-type="bigint(20)"/>
        </id>
        <property name="courseName">
            <column name="COURSE_NAME" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="courseCredit">
            <column name="COURSE_CREDIT" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>