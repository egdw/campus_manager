<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.ReciverEntity" table="RECIVER" schema="campus_travle">
        <id name="reciverId">
            <column name="RECIVER_ID" sql-type="bigint(20)"/>
        </id>
        <property name="postCode">
            <column name="POST_CODE" sql-type="varchar(255)"/>
        </property>
        <property name="postMessage">
            <column name="POST_MESSAGE" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="postCreatedate">
            <column name="POST_CREATEDATE" sql-type="datetime" not-null="true"/>
        </property>
        <property name="postLocation">
            <column name="POST_LOCATION" sql-type="varchar(255)" not-null="true"/>
        </property>
        <many-to-one name="packageCompanyByPostCompany" class="im.hdy.model.PackageCompanyEntity">
            <column name="POST_COMPANY" not-null="true"/>
        </many-to-one>
        <many-to-one name="postHouseByPostHouse" class="im.hdy.model.PostHouseEntity">
            <column name="POST_HOUSE" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>