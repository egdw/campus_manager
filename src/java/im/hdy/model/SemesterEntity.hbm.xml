<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.SemesterEntity" table="SEMESTER" schema="campus_travle">
        <id name="semesterId">
            <column name="SEMESTER_ID" sql-type="int(11)"/>
        </id>
        <property name="semesterName">
            <column name="SEMESTER_NAME" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>