<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="im.hdy.model.ProvicesLocationEntity" table="PROVICES_LOCATION" schema="campus_travle">
        <id name="id">
            <column name="id" sql-type="bigint(6) unsigned"/>
        </id>
        <property name="pid">
            <column name="pid" sql-type="bigint(6) unsigned"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(1)"/>
        </property>
    </class>
</hibernate-mapping>